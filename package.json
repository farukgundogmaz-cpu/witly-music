{
  "name": "witly-music",
  "version": "1.0.0",
  "description": "Witly Music - Modern Music Streaming App",
  "main": "index.js",
  "scripts": {
    "android": "cd mobile && react-native run-android",
    "ios": "cd mobile && react-native run-ios",
